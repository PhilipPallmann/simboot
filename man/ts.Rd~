\name{ts}
\alias{ts}
\title{
  Internal function for simultaenous bootstrap intervals
}
\description{
  Internal function for simultaenous bootstrap intervals based on summed
  up counts for every species.
}
\usage{
ts(X, f, theta, cmat, conf.level, alternative, R, args)
}
\references{
  Beran, R. (1988) \emph{Balanced simultaneous confidence
    sets}. Journal of the American Statistical Association, 83, 679--686.
}
\note{
  Only internal function. Use function \code{\link{sbci}} instead
}
\keyword{ htest }
